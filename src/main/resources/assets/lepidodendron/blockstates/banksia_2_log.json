{
    "multipart": [
        {   "when": { "north": true },
            "apply": { "model": "lepidodendron:banksia_2_log_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "lepidodendron:banksia_2_log_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": true },
            "apply": { "model": "lepidodendron:banksia_2_log_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": true },
            "apply": { "model": "lepidodendron:banksia_2_log_side", "y": 270, "uvlock": true }
        },
        {   "when": { "up": true, "shoot": false },
            "apply": { "model": "lepidodendron:banksia_2_log_side", "x": 270, "uvlock": true }
        },
        {   "when": { "up": true, "shoot": true },
            "apply": { "model": "lepidodendron:banksia_2_log_shoot", "x": 270, "uvlock": true }
        },
        {   "when": { "down": true },
            "apply": { "model": "lepidodendron:banksia_2_log_side", "x": 90, "uvlock": true }
        },
        {   "when": { "north": false },
            "apply": { "model": "lepidodendron:banksia_2_log_noside" }
        },
        {   "when": { "east": false },
            "apply": { "model": "lepidodendron:banksia_2_log_noside", "y": 90, "uvlock": true }
        },
        {   "when": { "south": false },
            "apply": { "model": "lepidodendron:banksia_2_log_noside", "y": 180, "uvlock": true }
        },
        {   "when": { "west": false },
            "apply": { "model": "lepidodendron:banksia_2_log_noside", "y": 270, "uvlock": true }
        },
        {   "when": { "up": false },
            "apply": { "model": "lepidodendron:banksia_2_log_noside", "x": 270, "uvlock": true }
        },
        {   "when": { "down": false },
            "apply": { "model": "lepidodendron:banksia_2_log_noside", "x": 90, "uvlock": true }
        },
        {   "when": { "up": false },
            "apply": { "model": "lepidodendron:banksia_2_log_noside", "x": 270, "uvlock": true }
        },

        {   "when": {"shoot": true },
            "apply": { "model": "lepidodendron:banksia_2_leaves", "uvlock": true }
        }
    ]
}
